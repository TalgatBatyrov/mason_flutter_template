    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch IOS",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
        },
        {
            "name": "Launch development",
            "request": "launch",
            "type": "dart",
            "program": "lib/shared/config/flavors/main_development.dart",
            "args": [
                "--flavor",
                "development",
                "lib/shared/config/flavors/main_development.dart"
            ]
        },
        {
            "name": "Launch production",
            "request": "launch",
            "type": "dart",
            "program": "lib/shared/config/flavors/main_production.dart",
            "args": [
                "--flavor",
                "production",
                "lib/shared/config/flavors/main_production.dart"
            ]
        },
        {
            "name": "Launch staging",
            "request": "launch",
            "type": "dart",
            "program": "lib/shared/config/flavors/main_staging.dart",
            "args": [
                "--flavor",
                "staging",
                "lib/shared/config/flavors/main_staging.dart"
            ]
        }
    ]
}